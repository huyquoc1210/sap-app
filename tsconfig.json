{
  "compilerOptions": {
    "target": "ES5",
    "module": "AMD",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": false,
    "rootDir": "./src",
    "outDir": "./webapp",
    "baseUrl": "./",
    "paths": {
      "ui5/walkthrough/*": ["webapp/*"]
    }
  },
  "include": ["./src/**/*"],
  "exclude": ["./test"]
}
